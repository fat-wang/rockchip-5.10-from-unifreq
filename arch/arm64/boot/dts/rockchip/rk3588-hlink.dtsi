/* USB30_2 | pcie2x1l1 | sata2 => combphy2_psu
 * usbhost3_0 + usbhost_dwc3_0 | pcie2x1l1 | sata2
 * combphy2_psu
 */
&usbhost3_0 {
	status = "disabled";
};

&usbhost_dwc3_0 {
	status = "disabled";
};

/* rtl8125b 2.5g ethernet */
&pcie2x1l1 {
	reset-gpios = <&gpio4 RK_PA2 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&sata2 {
	status = "disabled";
}; 

&combphy2_psu {
	status = "okay";
};

/* pce2x1l0 | sata1 => combphy1_ps */
/* m.2 slot for pcie wifi module or extend rtl8125b board */
&pcie2x1l0 {
	reset-gpios = <&gpio0 RK_PB0 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&sata1 {
	status = "disabled";
}; 

&combphy1_ps {
	status = "okay";
};

/* pcie3x4 => pcie30phy */
/* H88K v1 & v2: pcie x4 slot */
/* H88K v3: m.2 nvme */
&pcie3x4 {
	reset-gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie30>;
	status = "okay";
};

&pcie30phy {
	rockchip,pcie30-phymode = <PHY_MODE_PCIE_AGGREGATION>;
	status = "okay";
};

/* pcie2x1l2 | sata0 => combphy0_ps */
/* H88K V1 & V2 : sata0 enabled */
/* H88K V3 : pcie2x1l2 enabled */
&pcie2x1l2 {
	status = "disabled";
};

&sata0 {
	status = "disabled";
};

&combphy0_ps {
	status = "disabled";
};

